[Unit]
Description=Yumemi
After=network.target

[Service]
User=www-data
Group=www-data
EnvironmentFile=/srv/kazamatsuri/yumemi/env
ExecStart=/srv/kazamatsuri/yumemi/bin/hubot -a discord
WorkingDirectory=/srv/kazamatsuri/yumemi
TimeoutSec=10
Restart=always
RestartSec=1

[Install]
WantedBy=multi-user.target
